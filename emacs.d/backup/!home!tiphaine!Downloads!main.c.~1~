#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void my_putchar(char c);
void my_putstr(char *str);
int  my_strlen(char *str);
int  my_getnbr(char *str);
void my_putnbr(int nb);
int  my_is_neg(int n);
int  my_is_even(int n);
int  my_is_odd(int n);
void my_swap(int *a, int *b);
char *my_strcpy(char *dest, char *src);
char *my_strncpy(char *dest, char *src, int n);
int  my_strcmp(char *s1, char *s2);
int  my_strncmp(char *s1, char *s2, int n);
char *my_strcat(char *str1, char *str2);
char *my_strncat(char *str1, char *str2, int n);
char *my_strstr(char *str, char *to_find);

char *my_strupcase(char *str);
char *my_strlowcase(char *str);
int my_nbrlen(int n);
int my_nbrlen_base(int n, int base);
void my_putnbr_base(int n, char *base);
int my_getnbr_base(char *str, char *base);
char *my_strdup(char *);
char **my_str_to_wordtab(char *);

static void _putchar(char c)
{
	printf("my_putchar(\'%c\') => ", c);
	fflush(stdout);
	my_putchar(c);
	printf("\n");
}

static void _putstr(char *str)
{
	printf("my_putstr(\"%s\") => \"", str);
	fflush(stdout);
	my_putstr(str);
	printf("\"\n");
}

static void _strlen(char *str)
{
	printf("my_strlen(\"%s\") => %d\n", str, my_strlen(str));
}

static void _getnbr(char *str)
{
	printf("my_getnbr(\"%s\") => %d\n", str, my_getnbr(str));
}

static void _putnbr(int n)
{
	printf("my_putnbr(%d) => ", n);
	fflush(stdout);
	my_putnbr(n);
	printf("\n");
}

static void _is_neg(int n)
{
	printf("my_is_neg(%d) => %d\n", n, my_is_neg(n));
}

static void _is_even(int n)
{
	printf("my_is_even(%d) => %d\n", n, my_is_even(n));
}

static void _is_odd(int n)
{
	printf("my_is_odd(%d) => %d\n", n, my_is_odd(n));
}

static void _swap(int a, int b)
{
	printf("{a: %d, b: %d} => ", a, b);
	my_swap(&a, &b);
	printf("my_swap(&a, &b) => {a: %d, b: %d}\n", a, b);
}

static void _strcpy(char *src)
{
	char s[512] = {'\0'};

	printf("my_strcpy(\"%s\", \"%s\") => \"%s\"\n", s, src, my_strcpy(s, src));
}

static void _strncpy(char *src, int n)
{
	char s[512] = {'\0'};

	printf("my_strncpy(\"%s\", \"%s\", %d) => \"%s\"\n", s, src, n, my_strncpy(s, src, n));
}

static void _strcmp(char *s1, char *s2)
{
	int res = my_strcmp(s1, s2);

	printf("my_strcmp(\"%s\", \"%s\") => %c\n", s1, s2, res ? res < 0 ? '<' : '>' : '=');
}

static void _strncmp(char *s1, char *s2, int n)
{
	int res = my_strncmp(s1, s2, n);

	printf("my_strncmp(\"%s\", \"%s\", %d) => %c\n", s1, s2, n, res ? res < 0 ? '<' : '>' : '=');
}

static void _strcat(char *dest, char *src)
{
	char s[128] = {'\0'};

	strcpy(s, dest);
	printf("my_strcat(\"%s\", \"%s\") => \"%s\"\n", dest, src, my_strcat(s, src));
}

static void _strncat(char *dest, char *src, int n)
{
	char s[128] = {'\0'};

	strcpy(s, dest);
	printf("my_strncat(\"%s\", \"%s\", %d) => \"%s\"\n", dest, src, n, my_strncat(s, src, n));
}

static void _strstr(char *str, char *to_find)
{
	char *ret = NULL;

	printf("my_strstr(\"%s\", \"%s\") => ", str, to_find);
	ret = my_strstr(str, to_find);
	printf("\"%s\" [%ld]\n", ret, ret ? ret - str : -1);
}

static void _strupcase(char *str)
{
	char *ret;

	str = strdup(str);
	printf("my_strupcase(\"%s\") => ", str);
	ret = my_strupcase(str);
	printf("\"%s\" [%s]\n", str, ret == str ? "OK" : "KO");
	free(str);
}

static void _strlowcase(char *str)
{
	char *ret;

	str = strdup(str);
	printf("my_strlowcase(\"%s\") => ", str);
	ret = my_strlowcase(str);
	printf("\"%s\" [%s]\n", str, ret == str ? "OK" : "KO");
	free(str);
}

static void _nbrlen(int n)
{
	printf("my_nbrlen(%d) => %d\n", n, my_nbrlen(n));
}

static void _nbrlen_base(int n, int base)
{
	printf("my_nbrlen_base(%d, %d) => %d\n", n, base, my_nbrlen_base(n, base));
}

static void _putnbr_base(int n, char *base)
{
	printf("my_putnbr_base(%d, \"%s\") => ", n, base);
	fflush(stdout);
	my_putnbr_base(n, base);
	printf("\n");
}

static void _getnbr_base(char *str, char *base)
{
	printf("my_getnbr_base(\"%s\", \"%s\") => %d\n", str, base, my_getnbr_base(str, base));
}

static void _strdup(char *s)
{
	char *sdup = my_strdup(s);

	printf("my_strdup(\"%s\") => \"%s\" [%d]\n", s, sdup, s != sdup);
}

static void _str_to_wordtab(char *s)
{
	char **arr = my_str_to_wordtab(s);
	char **_arr = arr;

	if (!arr)
		return ;

	printf("my_str_to_wordtab(\"%s\") => {", s);
	if (*arr) {
		printf("\"%s\"", *arr);
		while (*++arr)
			printf(", \"%s\"", *arr);
	}
	printf("}\n");
	free(_arr);
}

