#include <unistd.h>

void my_putchar(char)
{
	write(1, &c, 1);
}

void my_print_comb2(void)
{
	for (int a = 0; a <= 98; ++a)
		for (int b = a + 1; b <= 99; ++b) {
			if (a > 0 || b > 1) {
				my_putchar(',');
				my_putchar(' ');
			}
			my_putchar('0' + a / 10);
			my_putchar('0' + a % 10);
			my_putchar(' ');
			my_putchar('0' + b / 10);
			my_putchar('0' + b % 10);
		}
	my_putchar('\n');
}

int main()
{
	my_print_comb2();
	return 0;
}
