[requires]
portaudio/v190600.20161030@bincrafters/stable
opus/1.2.1@bincrafters/stable
Qt/5.11.1@bincrafters/stable
boost_asio/1.65.1@bincrafters/stable
boost_serialization/1.65.1@bincrafters/stable

[generators]
cmake

[options]
portaudio:shared=True
opus:shared=True
Qt:shared=True
Qt:qtx11extras=True

[imports]
bin, *.dll -> ../bin # Copies all dll files from packages bin folder to my "bin" folder
lib, *.dylib -> ../bin # Copies all dylib files from packages lib folder to my "bin" folder
